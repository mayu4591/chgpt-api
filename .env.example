# 環境変数設定例
# このファイルを .env として保存して使用

# サーバー設定
PORT=8000
HOST=0.0.0.0

# ブラウザ設定
BROWSER_TYPE=chrome
HEADLESS=false
TIMEOUT=30

# ChatGPT設定
CHATGPT_URL=https://chat.openai.com

# SeleniumWrapper設定
CHROME_PATH="C:\Program Files\Google\Chrome\Application\chrome.exe"
PROFILE_DIR_PATH=""
CHROME_DEBUG_PORT=""

# 自動起動設定
AUTO_START_BROWSER=true
STARTUP_TIMEOUT=30

# 入力処理最適化設定
INPUT_CLEANUP_DELAY=0.8
SAFE_SEND_LIMIT=150
INIT_TIMEOUT=15
RESIDUAL_CLEANUP=true

# ブラウザ自動起動設定
AUTO_START_BROWSER=true
STARTUP_TIMEOUT=30

# ログ設定
LOG_LEVEL=INFO
